# Copy this file to .env and modify as needed

# Server configuration
PORT=8080

# Environment type (development, testing, production)
ENVIRONMENT=development

# Optional: Custom hostname (defaults to 0.0.0.0)
# HOSTNAME=localhost

# Optional: Log level (trace, debug, info, notice, warning, error, critical)
LOG_LEVEL=info